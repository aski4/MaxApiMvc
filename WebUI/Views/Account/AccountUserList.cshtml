<div class="panel panel-primary">
    <table class="table table-striped">
        <thead>
            <tr><th>UserName</th><th>Email</th><th>Address</th><th>User</th><th>Admin</th></tr>
        </thead>
        <tbody data-bind="foreach: adminModel.users()">
            <tr>
                <td data-bind="text: $data.UserName"></td>
                <td data-bind="text: $data.Email"></td>
                <td data-bind="text: $data.Address"></td>
                <td data-bind="text: $data.Roles[0]"></td>
                <td data-bind="text: $data.Roles[1]"></td>
            </tr>
        </tbody>
    </table>
</div>
<div class="text-center">
    <button class="btn btn-primary"
            data-bind="click: setListMode.bind($data, 'addProduct')">
        Add Product
    </button>
</div>
